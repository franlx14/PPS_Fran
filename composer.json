{
    "name": "franlx14/pps_fran",
    "description": "A short description of my project",
    "require": {
        "php": "^8.3",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "MyProject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.3.0"
        },
        "process-timeout": 900
    },
    "license": "proprietary",
    "scripts": {
        "post-install-cmd": [
            "@composer install"
        ]
    }
}

  